{
    "name": "Odoo Barcode",
    "description": "Simulate barcode scanning!",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "/images/icon.png"
          }
    },
    "icons": {
        "128": "/images/icon.png"
    }
}
